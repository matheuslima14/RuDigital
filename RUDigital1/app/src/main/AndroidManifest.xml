<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.usuario.rudigital">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/rest"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TelaLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TelaCadastro" />
        <activity android:name=".TelaInicial" />
        <activity
            android:name=".TelaMenu"
            android:label="@string/menuInicial"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TelaVotarCardapio"
            android:label="@string/votarCardapio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TelaAvisarComer"
            android:label="@string/avisarComer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TelaAvaliarRefeicao"
            android:label="@string/avaliarRefeicao"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>